1 VERSIONINFO
 FILEVERSION ${DCVIZ_MAJOR_VERSION}, ${DCVIZ_MINOR_VERSION}, ${DCVIZ_PATCH_VERSION}, ${DCVIZ_SOVERSION}
 PRODUCTVERSION ${DCVIZ_MAJOR_VERSION}, ${DCVIZ_MINOR_VERSION}, ${DCVIZ_PATCH_VERSION}, ${DCVIZ_SOVERSION}
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x0L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription", "DCViz Binary"
            VALUE "FileVersion", "${DCVIZ_MAJOR_VERSION}, ${DCVIZ_MINOR_VERSION}, ${DCVIZ_PATCH_VERSION}, ${DCVIZ_SOVERSION}"
            VALUE "InternalName", "DCViz"
            VALUE "LegalCopyright", "Copyright (C) 2018"
            VALUE "OriginalFilename", ""
            VALUE "ProductName", "DCViz"
            VALUE "ProductVersion", "${DCVIZ_MAJOR_VERSION}, ${DCVIZ_MINOR_VERSION}, ${DCVIZ_PATCH_VERSION}, ${DCVIZ_SOVERSION}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
