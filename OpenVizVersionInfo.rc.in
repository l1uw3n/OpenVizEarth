1 VERSIONINFO
 FILEVERSION ${OpenVIZ_MAJOR_VERSION}, ${OpenVIZ_MINOR_VERSION}, ${OpenVIZ_PATCH_VERSION}, ${OpenVIZ_SOVERSION}
 PRODUCTVERSION ${OpenVIZ_MAJOR_VERSION}, ${OpenVIZ_MINOR_VERSION}, ${OpenVIZ_PATCH_VERSION}, ${OpenVIZ_SOVERSION}
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x0L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription", "OpenVIZ Binary"
            VALUE "FileVersion", "${OpenVIZ_MAJOR_VERSION}, ${OpenVIZ_MINOR_VERSION}, ${OpenVIZ_PATCH_VERSION}, ${OpenVIZ_SOVERSION}"
            VALUE "InternalName", "OpenVIZ"
            VALUE "LegalCopyright", "Copyright (C) 2018"
            VALUE "OriginalFilename", ""
            VALUE "ProductName", "OpenVIZ"
            VALUE "ProductVersion", "${OpenVIZ_MAJOR_VERSION}, ${OpenVIZ_MINOR_VERSION}, ${OpenVIZ_PATCH_VERSION}, ${OpenVIZ_SOVERSION}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
